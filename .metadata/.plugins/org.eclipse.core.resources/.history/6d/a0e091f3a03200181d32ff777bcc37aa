package moskitoAttack;

public class Humain extends Agents {
	

	public Humain () {
		super ();
	}
	
	public Humain (int i, int j) {
		super (i,j);
	}
	
		
	
	@Override
	public String toString () {
		if (this.sexe == 'f') { 
			if (this.infecte == true) {
				return "G";
			}
			else {
				return "g";
			}
		}
		else {
			if (this.infecte == true) {
				return "B";
			}
			else {
				return "b";
			}
		}
	}
}
